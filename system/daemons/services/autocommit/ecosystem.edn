{:apps [{:name "autocommit"
         :script "pnpm"
         :cwd "/home/err/devel/promethean"
         :args ["autocommit"
                "--path" "../"
                "-r"
                "--handle-subrepos"
                "--subrepo-strategy" "seperate"
                "--debounce-ms" "10000"
                "--model" "error/qwen3:4b-instruct-100k"
                "--base-url" "http://localhost:11434"]
         :env {:OPENAI_BASE_URL "http://localhost:11434"
               :AUTOCOMMIT_MODEL "error/qwen3:4b-instruct-100k"
               :NODE_ENV "production"}
         :instances 1
         :interpreter "/usr/bin/env"
         :autorestart true
         :watch ["./packages/autocommit/dist"]
         :error_file "/home/err/devel/promethean/logs/autocommit-error.log"
         :out_file "/home/err/devel/promethean/logs/autocommit-out.log"
         :log_file "/home/err/devel/promethean/logs/autocommit.log"
         :time true
         :kill_timeout 5000
         :restart_delay 5000
         :max_restarts 10
         :min_uptime 10000}]}
