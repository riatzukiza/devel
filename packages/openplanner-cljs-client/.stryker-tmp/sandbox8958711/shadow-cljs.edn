{:source-paths ["src" "test"]

 :dependencies []

 :builds
 {:lib
  {:target :esm
   :output-dir "dist"
   :modules {:index {:entries [promethean.openplanner.client]
                     :exports {:createOpenPlannerClient promethean.openplanner.client/create-openplanner-client
                               :defaultOpenPlannerConfig promethean.openplanner.client/default-config
                               :createOpenPlannerEvent promethean.openplanner.client/create-openplanner-event}}}
   :compiler-options {:output-feature-set :es-next}}

  :test
  {:target :node-script
   :output-to "target/test.cjs"
   :main promethean.openplanner.client-test/-main
   :compiler-options {:output-feature-set :es-next}}}}
