{
  "name": "devel",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "node scripts/nx-affected.mjs test",
    "test:all": "pnpm exec nx run-many --target=test --all",
    "test:octavia": "pnpm run build:octavia && vitest run --config vitest.config.ts --threads false",
    "test:octavia:coverage": "pnpm run build:octavia && c8 vitest run --config vitest.config.ts --threads false",
    "stryker:octavia": "pnpm run build:octavia && stryker run stryker.octavia.conf.json",
    "build": "node scripts/nx-affected.mjs build",
    "build:all": "pnpm exec nx run-many --target=build --all",
    "build:octavia": "pnpm --filter @promethean-os/lmdb-cache build && tsc -p tsconfig.octavia.json",
    "lint": "node scripts/nx-affected.mjs lint",
    "lint:all": "pnpm exec nx run-many --target=lint --all",
    "typecheck": "node scripts/nx-affected.mjs typecheck",
    "typecheck:all": "pnpm exec nx run-many --target=typecheck --all",
    "giga:watch": "bun run src/giga/giga-watch.ts",
    "giga:commit": "bun run src/giga/commit-propagator.ts",
    "pantheon:msg": "bun run src/giga/pantheon.ts",
    "issues:project": "bun run src/issues/projector.ts",
    "inbox:pipeline": "node scripts/inbox-pipeline.mjs",
    "inbox:meta-pipeline": "node scripts/meta-agent-evaluator.mjs",
    "giga:nx:generate": "bun run src/giga/generate-nx-projects.ts",
    "giga:plugin:bundle": "bun run tools/nx-plugins/giga/bundle-plugin.ts",
    "giga:affected": "nx affected --target=test --build",
    "lisp-fixer:build": "pnpm --filter @promethean-os/lisp-fixer build",
    "lisp-fixer:lint": "pnpm --filter @promethean-os/lisp-fixer lint",
    "lisp-fixer:typecheck": "pnpm --filter @promethean-os/lisp-fixer typecheck",
    "lisp-fixer:test": "pnpm --filter @promethean-os/lisp-fixer test",
    "lisp-fixer:scan": "pnpm --filter @promethean-os/lisp-fixer scan",
    "lisp-fixer:mutate": "pnpm --filter @promethean-os/lisp-fixer mutate",
    "lisp-fixer:dataset": "pnpm --filter @promethean-os/lisp-fixer dataset",
    "lisp-fixer:infer": "pnpm --filter @promethean-os/lisp-fixer infer",
    "lisp-fixer:demo": "pnpm --filter @promethean-os/lisp-fixer demo",
    "nx:publish:affected": "bun run src/nx-publish-affected.ts",
    "lint:md": "markdownlint-cli2",
    "lint:md:notes": "markdownlint-cli2 docs/notes/**/*.md",
    "hooks:install": "bash ./bin/install-pre-push-hooks.sh",
    "postinstall": "pnpm run hooks:install || true; pnpm run build:octavia || true",
    "lint:md:agent": "node scripts/mdlint-agent.mjs --batch 50 --max-iter 8",
    "lint:md:agent:sdk": "node scripts/mdlint-agentsdk.mjs --batch 50 --max-iter 8",
    "ecosystem:generate": "pnpm --dir orgs/riatzukiza/promethean --filter @promethean-os/ecosystem-dsl run generate -- --dir ../../../../../system/daemons --output ../../../../../ecosystem.config.enhanced.mjs",
    "ecosystem:watch": "pnpm --dir orgs/riatzukiza/promethean --filter @promethean-os/ecosystem-dsl run generate:watch -- --dir ../../../../../system/daemons --output ../../../../../ecosystem.config.enhanced.mjs",
    "ecosystem:validate": "pnpm --dir orgs/riatzukiza/promethean --filter @promethean-os/ecosystem-dsl run validate -- --dir ../../../../../system/daemons",
    "index-opencode-sessions": "tsx index_opencode_sessions.ts",
    "search-opencode-sessions": "tsx search_opencode_sessions.ts",
    "reconstitute": "tsx reconstitute.ts",
    "generate-ecosystem": "npx shadow-cljs release clobber"
  },
  "bin": {
    "octavia": "dist/octavia/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@opencode-ai/sdk": "^1.1.36",
    "@promethean-os/lmdb-cache": "*",
    "chromadb": "^3.2.2",
    "commander": "^14.0.2",
    "level": "^10.0.0",
    "redis": "^5.9.0",
    "shadow-cljs": "^3.3.5"
  },
  "devDependencies": {
    "@nx/devkit": "^20.0.0",
    "@nx/js": "^20.0.0",
    "@openai/agents": "^0.2.1",
    "@promethean-os/ecosystem-dsl": "file:orgs/riatzukiza/promethean/cli/ecosystem-dsl",
    "@types/node": "^20.11.30",
    "c8": "^8.0.0",
    "dotenv": "^17.2.3",
    "markdownlint": "^0.39.0",
    "markdownlint-cli2": "^0.19.0",
    "nx": "^20.0.0",
    "openai": "^4.77.0",
    "stryker": "^0.35.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^0.34.5",
    "zod": "^3.23.8"
  },
  "trustedDependencies": [
    "clj-kondo"
  ]
}
