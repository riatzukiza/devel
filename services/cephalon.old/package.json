{
  "name": "@promethean-os/cephalon",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Cephalon - Always-running mind with vector memory, persistent memory, and event subscriptions",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "policy"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && ava",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava",
    "format": "pnpm exec prettier --write .",
    "dev": "tsx watch src/main.ts",
    "start": "tsx src/main.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/static": "^9.0.0",
    "@promethean-os/event": "workspace:*",
    "@promethean-os/persistence": "workspace:*",
    "chromadb": "^1.8.0",
    "discord.js": "^14.14.0",
    "dotenv": "^16.3.0",
    "edn-data": "^1.1.2",
    "fastify": "^5.7.2",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "ava": "^6.0.0",
    "c8": "^9.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=20.0.0"
  }
}
