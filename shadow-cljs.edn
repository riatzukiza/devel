 :js-options {:entry-keys ["module" "browser" "main"]
             :js-provider :require}

 :nrepl {:port 9000
          :host "0.0.0.0"}

 :jvm-opts ["-Xmx4g" "-Xss2m" "-XX:+UseG1GC" "-XX:ReservedCodeCacheSize=64m"]

 :js-package-dirs ["node_modules" "services/sentinel/node_modules"]

 :npm-deps {:react "18.3.1"
             :react-dom "18.3.1"}

 :builds
 {:clobber {:target :node-script
            :main index/-main
            :output-to ".clobber/index.cjs"
            :compiler-options {:infer-externs :auto
                               :source-map true}}

  :cephalon-cljs/cephalon {:target :node-script
                            :main promethean.main/-main
                            :output-to "packages/cephalon-cljs/dist/cephalon.js"
                            :compiler-options {:externs ["externs.js"]}
                            :js-options {:js-provider :require}
                            :devtools {:repl-pprint true}}

  :openhax/opencode-reactant/app {:target :browser
                                :output-dir "orgs/open-hax/openhax/packages/opencode-reactant/resources/public/js"
                                :asset-path "/js"
                                :modules {:main {:init-fn opencode.ui.core/init}}
                                :compiler-options {:warnings-as-errors true
                                                   :elide-asserts false}
                                :devtools {:http-root "orgs/open-hax/openhax/packages/opencode-reactant/resources/public"
                                           :http-port 8700
                                           :shadow-port 7890
                                           :hud {:errors :warnings
                                                 :progress true}
                                           :autoload true
                                           :preloads [devtools.preload]}
                                :build-hooks [(shadow.cljs.build-report/hook {:output-to "orgs/open-hax/openhax/packages/opencode-reactant/target/build-report.html"})]
                                :closure-defines {opencode.ui.state/REPO_SLUG #shadow/env "REPO_SLUG"}}

  :openhax/opencode-reactant/test {:target :node-script
                                  :output-to "orgs/open-hax/openhax/packages/opencode-reactant/target/test.js"
                                  :main opencode.test-runner/-main}}

  :openhax/opencode-cljs-plugin-nrepl-eval/opencode {:target :esm
                                                    :output-dir "orgs/open-hax/opencode-cljs-plugin-nrepl-eval/dist"
                                                    :modules {:plugin {:entries [my.opencode.entry]}}
                                                    :compiler-options {:output-feature-set :es2022}}

  :openhax/museeks/cli {:target :node-script
                        :main museeks.core/-main
                        :output-to "orgs/open-hax/museeks/bin/mu"
                        :output-dir "orgs/open-hax/museeks/target/cli"
                        :optimizations :advanced
                        :pretty-print false
                        :source-map true
                        :closure-defines {museeks.config/version "0.1.0"}
                        :devtools {:before-load-async museeks.core/stop
                                   :after-load museeks.core/start}}

  :openhax/workbench/app {:target :browser
                        :output-dir "orgs/open-hax/workbench/public/renderer"
                        :asset-path "/renderer"
                        :modules {:main {:init-fn opencode-unified.main/main}}
                        :dev {:compiler-options {:closure-defines {re-frame.trace/trace-enabled? true
                                                                   day8.re-frame.tracing/trace-enabled? true}}}
                        :devtools {:http-root "orgs/open-hax/workbench/public" :http-port 8080}}

  :openhax/workbench/renderer {:target :browser
                              :output-dir "orgs/open-hax/workbench/dist/renderer"
                              :modules {:main {:init-fn renderer/init}}
                              :devtools {:http-root "orgs/open-hax/workbench/dist/renderer"
