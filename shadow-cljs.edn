                                                     :watch-include ["orgs/riatzukiza/promethean/services/frontend-service/src/cljs"]
                                                     :autoload true
                                                     :before-load-async promethean.frontend-service.core/stop!
                                                     :after-load promethean.frontend-service.core/start!}
                                          :build-hooks [(shadow.cljs.build-report/hook {:output-to "orgs/riatzukiza/promethean/services/frontend-service/target/report.html"})]
                                          :dev {:compiler-options {:source-map-detail-level :all
                                                             :pseudo-names true
                                                             :pretty-print true}}
                                          :release {:compiler-options {:optimizations :advanced
                                                                 :elide-asserts true
                                                                 :source-map false
                                                                 :infer-externs :auto}}}

  :promethean/frontend-service/frontend-service-test {:target :node-script
